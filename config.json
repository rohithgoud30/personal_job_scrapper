  ],
  "sites": [
    {
      "key": "kforce",
      "host": "kforce.com",
      "userDataDir": ".playwright/kforce",
      "login": {
        "required": false,
        "url": "https://www.kforce.com/login/",
        "email": "",
        "password": "",
        "selectors": {
          "openLogin": "",
          "email": "input[type=email]",
          "password": "input[type=password]",
          "submit": "button[type=submit]"
        }
      },
      "search": {
        "url": "https://www.kforce.com/find-work/search-jobs/",
        "criteria": {
          "searchKeywords": []
        },
        "postedTodayOnly": true,
        "jobTypeFilter": ["Contract"],
        "selectors": {
          "keywords": "input[placeholder='Search by Job Title or Skill']",
          "location": ".main-search-bar .Select input",
          "submit": ".main-search-bar .submitIcon",
          "card": ".data-jobs li",
          "title": "h2 a",
          "locationText": ".job-list-detail .locations",
          "posted": "h2 span",
          "next": ".data-job-pagination .btn-line-darkCerulean",
          "sortToggle": ".facets .Select-control",
          "sortOption": ".Select-option",
          "sortValueLabel": ".facets .Select-value-label",
          "sortOptionText": "Newest Jobs First",
          "jobType": ".job-list-detail .type",
          "jobTypeFacetOption": ".facetItem.TypeCode li",
          "jobTypeFacetText": "Contract"
        }
      },
      "run": {
        "maxPages": 2,
        "throttleSeconds": 8,
        "pageDelaySeconds": 30,
        "keywordDelaySeconds": 45
      },
      "disallowPatterns": [
        "apply-online",
        "/search-jobs/job-detail/apply-online.aspx",
        "apply-online.aspx"
      ],
      "cookieConsent": {
        "buttonSelectors": [
          "#onetrust-accept-btn-handler",
          "button#onetrust-accept-btn-handler",
          "button:has-text(\"Accept\")"
        ],
        "textMatches": ["Accept All Cookies", "Accept"],
        "waitForSeconds": 2
      }
    },
    {
      "key": "randstadusa",
      "host": "randstadusa.com",
      "userDataDir": ".playwright/randstadusa",
      "login": {
        "required": false,
        "url": "https://www.randstadusa.com/login/",
        "email": "",
        "password": "",
        "selectors": {
          "openLogin": "",
          "email": "input[type=email]",
          "password": "input[type=password]",
          "submit": "button[type=submit]"
        }
      },
      "search": {
        "url": "https://www.randstadusa.com/jobs/",
        "criteria": {
          "searchKeywords": [],
          "location": ""
        },
        "postedTodayOnly": true,
        "jobTypeFilter": ["Contract", "Temporary", "Temp to Perm"],
        "selectors": {
          "keywords": "input[placeholder*=\"job\" i], input[placeholder*=\"keyword\" i], input[name*=\"keyword\" i], input[type=\"search\"]",
          "location": "input[placeholder*=\"location\" i], input[name*=\"location\" i]",
          "submit": "button[type=\"submit\"], button:has-text(\"search\"), button:has-text(\"Search\")",
          "card": "ul.cards__list li.cards__item, article[data-automation-id], li[data-automation-id], .search-results article, .job-results article",
          "title": "h3.cards__title a.cards__link, a[href*=\"/jobs/\"]",
          "locationText": ".cards__meta-item:has(svg[aria-label*=\"location\" i]), .cards__meta-item, [data-automation-id*=\"location\"], .job-location, [class*=\"location\"]",
          "posted": ".cards__date, [data-automation-id*=\"posted\"], .job-date, time",
          "next": "button:has-text(\"Next\"), .pagination a[rel=\"next\"]",
          "sortToggle": "select#sortBy, select[name=\"sortBy\"], button:has-text(\"sort\"), button:has-text(\"sort:\"), button[aria-label*=\"sort\" i], [data-automation-id*=\"sort\"] button, .filters__sort button",
          "sortOption": "select#sortBy option, select[name=\"sortBy\"] option, [role=\"option\"], [role=\"menuitem\"], ul[role] li button, ul[role] li, li:has-text(\"date\"), button:has-text(\"date\")",
          "sortValueLabel": "select#sortBy, select[name=\"sortBy\"], button:has-text(\"sort:\"), button:has-text(\"date\"), button:has-text(\"relevancy\")",
          "sortOptionText": "date",
          "jobType": "",
          "jobTypeFacetOption": "",
          "jobTypeFacetText": ""
        }
      },
      "run": {
        "maxPages": 2,
        "throttleSeconds": 8,
        "pageDelaySeconds": 20,
        "keywordDelaySeconds": 30
      },
      "disallowPatterns": [
        "/apply/",
        "/job-apply-confirmation/",
        "/search-result/",
        "/subscriptions/",
        "/rna/",
        "/*/apply/"
      ],
      "cookieConsent": {
        "buttonSelectors": [
          "#onetrust-accept-btn-handler",
          "button#onetrust-accept-btn-handler",
          "button:has-text(\"Accept\")",
          "button:has-text(\"accept all cookies\")",
          "button:has-text(\"accept all\")"
        ],
        "textMatches": ["Accept All Cookies", "Accept"],
        "waitForSeconds": 2
      }
    }
  ]
}
